<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>KeepAlive</key>
  <true/>
  <key>Label</key>
  <string>{{launchagents_service_prefix}}.postgresql</string>
  <key>ProgramArguments</key>
  <array>
    <string>{{brew_path}}/opt/postgresql/bin/postgres</string>
    <string>-D</string>
    <string>{{brew_path}}/var/postgres</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
  <key>WorkingDirectory</key>
  <string>{{brew_path}}</string>
  <key>StandardOutPath</key>
  <string>{{brew_path}}/var/log/postgres.log</string>
  <key>StandardErrorPath</key>
  <string>{{brew_path}}/var/log/postgres.log</string>
  <key>UserName</key>
  <string>{{username}}</string>
</dict>
</plist>